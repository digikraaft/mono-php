{
    "name": "digikraaft/mono-php",
    "description": "PHP Implementation of Mono API",
    "keywords": [
        "digikraaft",
        "mono-php",
        "mono"
    ],
    "homepage": "https://github.com/digikraaft/mono-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Oladoyinbo",
            "email": "hello@digikraaft.ng",
            "homepage": "https://digikraaft.ng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Digikraaft\\Mono\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digikraaft\\Mono\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
